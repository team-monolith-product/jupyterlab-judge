{
    "name": "jupyterlab-judge",
    "version": "1.29.0",
    "description": "A simple online judge for Jupyter Lab.",
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/team-monolith-product/jupyterlab-judge",
    "bugs": {
        "url": "https://github.com/team-monolith-product/jupyterlab-judge/issues"
    },
    "license": "BSD-3-Clause",
    "author": {
        "name": "ChangHwan Lee",
        "email": "lch@team-mono.com"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
        "schema/*.json",
        "style/index.js"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/team-monolith-product/jupyterlab-judge.git"
    },
    "scripts": {
        "build": "jlpm build:lib && jlpm build:labextension:dev",
        "build:check": "tsc --noEmit",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
        "clean": "jlpm clean:lib",
        "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
        "clean:labextension": "rimraf jupyterlab_judge/labextension jupyterlab_judge/_version.py",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:lintcache": "rimraf .eslintcache .stylelintcache",
        "eslint": "jlpm eslint:check --fix",
        "eslint:check": "eslint . --cache --ext .ts,.tsx",
        "install:extension": "jlpm build",
        "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
        "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
        "prettier": "jlpm prettier:base --write --list-different",
        "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
        "prettier:check": "jlpm prettier:base --check",
        "stylelint": "jlpm stylelint:check --fix",
        "stylelint:check": "stylelint --cache \"style/**/*.css\"",
        "test": "jest --coverage",
        "watch": "run-p watch:src watch:labextension",
        "watch:labextension": "jupyter labextension watch .",
        "watch:src": "tsc -w --sourceMap"
    },
    "dependencies": {
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@jupyter/ydoc": "^1.0.2",
        "@jupyterlab/application": "~4.1.8",
        "@jupyterlab/apputils": "~4.2.8",
        "@jupyterlab/cells": "~4.1.8",
        "@jupyterlab/codeeditor": "~4.1.8",
        "@jupyterlab/codemirror": "~4.1.8",
        "@jupyterlab/coreutils": "~6.1.8",
        "@jupyterlab/docmanager": "~4.1.8",
        "@jupyterlab/docregistry": "~4.1.8",
        "@jupyterlab/filebrowser": "~4.1.8",
        "@jupyterlab/fileeditor": "~4.1.8",
        "@jupyterlab/launcher": "~4.1.8",
        "@jupyterlab/mainmenu": "~4.1.8",
        "@jupyterlab/observables": "~5.1.8",
        "@jupyterlab/outputarea": "~4.1.8",
        "@jupyterlab/property-inspector": "~4.1.8",
        "@jupyterlab/rendermime": "~4.1.8",
        "@jupyterlab/services": "~7.1.8",
        "@jupyterlab/settingregistry": "~4.1.8",
        "@jupyterlab/translation": "~4.1.8",
        "@types/codemirror": "~5.60.5",
        "react-query": "~3.39.0",
        "snakecase-keys": "^4.0.2"
    },
    "resolutions": {
        "yjs": "13.5.49",
        "y-protocols": "1.0.5",
        "lib0": "0.2.65",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@mui/material": "^5.13.6",
        "@jupyterlab/application": "~4.1.8",
        "@jupyterlab/apputils": "~4.2.8",
        "@jupyterlab/attachments": "~4.1.8",
        "@jupyterlab/cells": "~4.1.8",
        "@jupyterlab/codeeditor": "~4.1.8",
        "@jupyterlab/codemirror": "~4.1.8",
        "@jupyterlab/coreutils": "~6.1.8",
        "@jupyterlab/docmanager": "~4.1.8",
        "@jupyterlab/docregistry": "~4.1.8",
        "@jupyterlab/documentsearch": "~4.1.8",
        "@jupyterlab/filebrowser": "~4.1.8",
        "@jupyterlab/fileeditor": "~4.1.8",
        "@jupyterlab/launcher": "~4.1.8",
        "@jupyterlab/lsp": "~4.1.8",
        "@jupyterlab/mainmenu": "~4.1.8",
        "@jupyterlab/nbformat": "~4.1.8",
        "@jupyterlab/notebook": "~4.1.8",
        "@jupyterlab/observables": "~5.1.8",
        "@jupyterlab/outputarea": "~4.1.8",
        "@jupyterlab/property-inspector": "~4.1.8",
        "@jupyterlab/rendermime": "~4.1.8",
        "@jupyterlab/rendermime-interfaces": "~3.9.8",
        "@jupyterlab/services": "~7.1.8",
        "@jupyterlab/settingregistry": "~4.1.8",
        "@jupyterlab/statedb": "~4.1.8",
        "@jupyterlab/statusbar": "~4.1.8",
        "@jupyterlab/testing": "~4.1.8",
        "@jupyterlab/testutils": "~4.1.8",
        "@jupyterlab/toc": "~6.1.8",
        "@jupyterlab/translation": "~4.1.8",
        "@jupyterlab/ui-components": "~4.1.8"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.5.2",
        "@jupyterlab/testutils": "~4.1.8",
        "@types/jest": "^29.2.0",
        "@types/json-schema": "^7.0.11",
        "@types/react": "^18.0.26",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.2.0",
        "mkdirp": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.1",
        "source-map-loader": "^1.0.2",
        "style-loader": "^3.3.1",
        "stylelint": "^15.10.1",
        "stylelint-config-recommended": "^13.0.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-prettier": "^4.0.0",
        "typescript": "~5.1.6",
        "yjs": "^13.5.40"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "discovery": {
            "server": {
                "managers": [
                    "pip"
                ],
                "base": {
                    "name": "jupyterlab_judge"
                }
            }
        },
        "extension": true,
        "outputDir": "jupyterlab_judge/labextension",
        "schemaDir": "schema"
    },
    "eslintConfig": {
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:prettier/recommended"
        ],
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "tsconfig.json",
            "sourceType": "module"
        },
        "plugins": [
            "@typescript-eslint"
        ],
        "rules": {
            "@typescript-eslint/naming-convention": [
                "error",
                {
                    "selector": "interface",
                    "format": [
                        "PascalCase"
                    ],
                    "custom": {
                        "regex": "^I[A-Z]",
                        "match": true
                    }
                }
            ],
            "@typescript-eslint/no-unused-vars": [
                "warn",
                {
                    "args": "none"
                }
            ],
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-namespace": "off",
            "@typescript-eslint/no-use-before-define": "off",
            "@typescript-eslint/quotes": [
                "error",
                "single",
                {
                    "avoidEscape": true,
                    "allowTemplateLiterals": false
                }
            ],
            "curly": [
                "error",
                "all"
            ],
            "eqeqeq": "error",
            "prefer-arrow-callback": "error"
        }
    },
    "eslintIgnore": [
        "node_modules",
        "dist",
        "coverage",
        "**/*.d.ts",
        "tests",
        "**/__tests__",
        "ui-tests"
    ],
    "prettier": {
        "singleQuote": true,
        "trailingComma": "none",
        "arrowParens": "avoid",
        "endOfLine": "auto",
        "overrides": [
            {
                "files": "package.json",
                "options": {
                    "tabWidth": 4
                }
            }
        ]
    },
    "stylelint": {
        "extends": [
            "stylelint-config-recommended",
            "stylelint-config-standard",
            "stylelint-prettier/recommended"
        ],
        "rules": {
            "property-no-vendor-prefix": null,
            "selector-no-vendor-prefix": null,
            "selector-class-pattern": null,
            "value-no-vendor-prefix": null
        }
    }
}
